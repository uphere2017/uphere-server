{
  "name": "uphere-server",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=development nodemon --inspect server.js",
    "test": "mocha spec",
    "coveralls": "NODE_ENV=test YOURPACKAGE_COVERAGE=1 ./node_modules/.bin/mocha spec --require blanket --reporter mocha-lcov-reporter | ./node_modules/coveralls/bin/coveralls.js"
  },
  "dependencies": {
    "body-parser": "^1.17.1",
    "express": "^4.15.2",
    "mongodb": "^2.2.25",
    "mongoose": "^4.9.1",
    "morgan": "^1.8.1",
    "socket.io": "^1.7.3"
  },
  "devDependencies": {
    "blanket": "^1.2.3",
    "chai": "^3.5.0",
    "coveralls": "^2.12.0",
    "gulp": "^3.9.1",
    "gulp-livereload": "^3.8.1",
    "gulp-nodemon": "^2.2.1",
    "gulp-notify": "^3.0.0",
    "mocha": "^3.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.11.0",
    "supertest": "^3.0.0"
  },
  "config": {
    "blanket": {
      "pattern": [
        "server.js",
        "src"
      ],
      "data-cover-never": [
        "node_modules",
        "spec"
      ]
    }
  }
}
